		var plugins={"adaptiveContent":{"css":"\r\n.klascementDescription figure{\r\n    width: 25%;\r\n    height: 25%;\r\n    float: right;\r\n}\r\n\r\n.klascementObject {\r\n    padding: 2vw;\r\n    margin-top: 1vh;\r\n    border: 1px solid grey;\r\n    background: #0000ff08;\r\n}\r\n\r\n.collapseicon {\r\n    margin-right: 10px;\r\n    cursor: pointer;\r\n}\r\n\r\n.klascementContainer {\r\n    padding-left: 1.5vw;\r\n    padding-right: 1.5vw;\r\n}\r\n\r\n\/* Tooltip container *\/\r\n.klascementTooltip {\r\n    position: relative;\r\n    display: inline-block;\r\n    \/* border-bottom: 1px dotted black; \/* If you want dots under the hoverable text *\/\r\n}\r\n\r\n\/* Tooltip text *\/\r\n.klascementTooltip .klascementTooltiptext {\r\n    visibility: hidden;\r\n    width: 20vw;\r\n    background-color: black;\r\n    color: #fff;\r\n    text-align: center;\r\n    padding: 5px 0;\r\n    border-radius: 6px;\r\n\r\n    \/* Position the tooltip text - see examples below! *\/\r\n    position: absolute;\r\n    z-index: 1;\r\n    top: 100%;\r\n    right: 0%;\r\n}\r\n\r\n.klascementTooltip .klascementTooltiptext a, .klascementTooltip .klascementTooltiptext:hover {\r\n    color: #fff;\r\n}\r\n\r\n\/* Show the tooltip text when you mouse over the tooltip container *\/\r\n.klascementTooltip:hover .klascementTooltiptext {\r\n    visibility: visible;\r\n}","script":"\/**\r\n * Licensed to The Apereo Foundation under one or more contributor license\r\n * agreements. See the NOTICE file distributed with this work for\r\n * additional information regarding copyright ownership.\r\n\r\n * The Apereo Foundation licenses this file to you under the Apache License,\r\n * Version 2.0 (the \"License\"); you may not use this file except in\r\n * compliance with the License. You may obtain a copy of the License at:\r\n *\r\n * http:\/\/www.apache.org\/licenses\/LICENSE-2.0\r\n *\r\n * Unless required by applicable law or agreed to in writing, software\r\n * distributed under the License is distributed on an \"AS IS\" BASIS,\r\n * WITHOUT WARRANTIES OR CONDITIONS OF ANY KIND, either express or implied.\r\n\r\n * See the License for the specific language governing permissions and\r\n * limitations under the License.\r\n *\/\r\n\r\n\/** plugin model file for adaptive content **\/\r\n\r\nadaptiveContent.useOAuth = true;\r\nadaptiveContent.getKlascementToken = \"jwks\/get_token.php\";\r\nadaptiveContent.klascementApi = \"klascement\/klascement_api.php\";\r\n\r\nadaptiveContent.initPlugin = function() {\r\n    \/\/$(\"#adaptiveContentMain\").css(\"background-color\", \"rgba(255,0,0,128)\");\r\n    debugger;\r\n}\r\n\r\nadaptiveContent.pageChanged = (f => u => {\r\n    f.call(adaptiveContent,u);\r\n    \/\/$(\"#adaptiveContentMain\").css(\"background-color\", \"rgba(0,255,0,128)\");\r\n})(adaptiveContent.pageChanged);\r\n\r\n\/*\r\nadaptiveContent.drawBlock = (function(_super) {\r\n    return function() {\r\n        _super.apply(this, arguments);\r\n    }\r\n})(adaptiveContent.drawBlock);\r\n*\/\r\n\r\nadaptiveContent.drawScore = (function(_super) {\r\n    return function() {\r\n        _super.apply(this, arguments);\r\n        debugger;\r\n        const statements = arguments[0];\r\n        const interaction = arguments[1];\r\n        const url = arguments[2];\r\n        const type = arguments[3];\r\n        const interactionIndex = arguments[4];\r\n        const fullWidth = arguments[5];\r\n        if (statements.length > 0 && adaptiveContent.dashboardMode == \"false\")\r\n        {\r\n            \/\/ determine score in same way as in xerte\r\n            if (\r\n                interaction.getAttribute(\"interactionType\") !== \"groupings\" &&\r\n                interaction.getAttribute(\"interactionType\") !== \"opinionclasses\"\r\n            ) {\r\n                let score = 0;\r\n\r\n                score = statements[0].result.score.raw;\r\n            }\r\n            for (\r\n                let blockIndex = 0;\r\n                blockIndex < interaction.children.length;\r\n                blockIndex++\r\n            ) {\r\n                const block = interaction.children[blockIndex];\r\n                const scoreRange = block.getAttribute(\"conScoreBetween\");\r\n                const range = scoreRange.split(\",\");\r\n                if (range.length == 2) {\r\n                    lowerBound = range[0];\r\n                    upperBound = range[1];\r\n                    if (score >= lowerBound && score <= upperBound) {\r\n                        adaptiveContent.loadKlascement(url, type, interaction, interactionIndex, block, blockIndex);\r\n                    }\r\n                }\r\n            }\r\n        }\r\n    }\r\n})(adaptiveContent.drawScore);\r\n\r\nadaptiveContent.getData = function(token, query, page, callback){\r\n    let actualQuery = adaptiveContent.klascementApi + sessionParam + \"&\" + query + \"&page=\" + page;\r\n    let headers = {};\r\n    if (token !== null) {\r\n        headers = {\r\n            'Authorization':'Bearer ' + token['access_token'],\r\n            'X-API-Key': token['client_id']\r\n        }\r\n    }\r\n\r\n    console.log(\"Running KlasCement query \" + query);\r\n    $.ajax({\r\n        type: \"GET\",\r\n        url: actualQuery,\r\n        headers: headers,\r\n        dataType: \"text\",\r\n        success: function(data) { \r\n\t    try {\r\n\t        let json_data = JSON.parse(data); \r\n                \/\/ decode omschrijving en logobron->html\r\n\t\tfor (i=0; i<json_data.content.length; i++)\r\n\t\t{\r\n\t\t    json_data.content[i].omschrijving = atob(json_data.content[i].omschrijving);\r\n\t\t    json_data.content[i].logobron.html = atob(json_data.content[i].logobron.html);\r\n\t\t}\r\n\t\tcallback(null, json_data);\r\n            }\r\n\t    catch(e)\r\n\t    {\r\n\t        console.log(data);\r\n\t    }\r\n        },\r\n        error: function(data){\r\n            callback(\"Error retrieving objects from KlasCement (query=\" + query +\")\");\r\n        }\r\n    });\r\n}\r\n\r\nadaptiveContent.loadKlascement = function(url, type, interaction, interactionIndex, block, blockIndex) {\r\n    if (block.getAttribute(\"getKlascementContent\") != null && block.getAttribute(\"getKlascementContent\") == \"true\"\r\n        && block.getAttribute(\"klascementQuery\") != null && block.getAttribute(\"klascementQuery\") != \"\") {\r\n        \/\/ Ok, lets retrieve the content from Klascement\r\n        \/\/ We need a token, request it\r\n        if (adaptiveContent.useOAuth) {\r\n            $.ajax({\r\n                type: \"GET\",\r\n                url: adaptiveContent.getKlascementToken + sessionParam,\r\n                dataType: \"json\",\r\n                success: function(token) {\r\n                    \/\/ Retrieve the content\r\n                    adaptiveContent.getKlascementContent(token, url, type, interaction, interactionIndex, block, blockIndex);\r\n                },\r\n                error: function(data){\r\n                    console.log(\"Error retrieving token from KlasCement \" + $('div').html(data.responseText).text());\r\n                }\r\n            });\r\n        }\r\n        else\r\n        {\r\n            adaptiveContent.getKlascementContent(null, url, type, interaction, interactionIndex, block, blockIndex);\r\n        }\r\n    }\r\n}\r\n\r\nadaptiveContent.getKlascementContent = function(token, url, type, interaction, interactionIndex, block, blockIndex) {\r\n    let klascementQuery = block.getAttribute(\"klascementQuery\");\r\n    let limit = \"\";\r\n    if (block.getAttribute(\"klascementLimit\") != null && block.getAttribute(\"klascementLimit\") != \"\") {\r\n        limit = block.getAttribute(\"klascementLimit\");\r\n    }\r\n    let randomize = false;\r\n    if (block.getAttribute(\"klascementRandomize\") != null && block.getAttribute(\"klascementRandomize\") == \"true\") {\r\n        randomize = true;\r\n    }\r\n    if (randomize) {\r\n        klascementQuery += \"&random=true\";\r\n    }\r\n    if (limit != \"\") {\r\n        klascementQuery += \"&size=\" + limit;\r\n    }\r\n\r\n    const objects = [];\r\n    let page = 1;\r\n    adaptiveContent.getData(token, klascementQuery, page,\r\n        function getmorepages(err, data) {\r\n            if (err) {\r\n                console.log(err);\r\n            } else if (data != null && data.content.length > 0) {\r\n                objects.push(...data.content);\r\n                \/\/ Retrieve more records if needed\r\n                page += 1;\r\n                if (data.meta.total_elements > limit && data.meta.pagesize < limit) {\r\n                    adaptiveContent.getData(token, klascementQuery, page, getmorepages);\r\n                } else {\r\n                    const id = \"#\" + escapeUrl(url, type, interactionIndex);\r\n                    let title = x_currentPageXML.getAttribute(\"klascementIntroTitle\");\r\n                    if (title == null || title == \"\") {\r\n                        title = \"KlasCement\";\r\n                    }\r\n\r\n                    let intro = x_currentPageXML.getAttribute(\"klascementIntro\");\r\n                    if (intro == null || intro == \"\") {\r\n                        intro = \"<p>Hieronder staat een selectie van trainingen en workshops die je op Klascement kunt vinden om je te helpen om op een hoger niveau te komen.<\/p>\";\r\n                    }\r\n                    const collapseicon = $(\"<span>\")\r\n                        .attr(\"id\", \"klascementIcon\" + interactionIndex + \"_\" + blockIndex)\r\n                        .addClass(\"collapseicon\")\r\n                        .addClass(\"fa\")\r\n                        .click(function () {\r\n                            const collapsibleElement =\r\n                                $(this).parents(\".klascement\").find(\".collapsibleContent\");\r\n\r\n                            if (collapsibleElement.is(\":visible\")) {\r\n                                collapsibleElement.hide();\r\n                                $(this).removeClass(\"fa-x-acc-show\").addClass(\"fa-x-acc-hide\");\r\n                            } else {\r\n                                collapsibleElement.show();\r\n                                $(this).removeClass(\"fa-x-acc-hide\").addClass(\"fa-x-acc-show\");\r\n                            }\r\n                        });\r\n                    let collapse = true;\r\n                    if (block.getAttribute(\"klascementCollapse\") != null && block.getAttribute(\"klascementCollapse\") == \"false\") {\r\n                        collapseicon.addClass(\"fa-x-acc-show\");\r\n                        collapse = false;\r\n                    } else {\r\n                        collapseicon.addClass(\"fa-x-acc-hide\");\r\n                        collapse = true;\r\n                    }\r\n                    let klascementTitle = $(\"<h3>\")\r\n                        .append(collapseicon)\r\n                        .append(title);\r\n                    let klascementContent = $(\"<div class='klascement'>\")\r\n                        .append(klascementTitle);\r\n                    let klascementContainer = $(\"<div class=\\\"klascementContainer collapsibleContent\\\"><\/div>\");\r\n                    if (collapse) {\r\n                        klascementContainer.hide();\r\n                    }\r\n                    klascementContainer.append(intro);\r\n                    \/\/adaptiveContent.renderBlocks(objects, klascementContainer, block);\r\n                    const showDescription = block.getAttribute(\"klascementShowDescription\") == null || block.getAttribute(\"klascementShowDescription\") == \"true\";\r\n                    const showThumbnail = block.getAttribute(\"klascementShowThumbnail\") == null || block.getAttribute(\"klascementShowThumbnail\") == \"true\";\r\n                    const showType = block.getAttribute(\"klascementShowType\") == null || block.getAttribute(\"klascementShowType\") == \"true\";\r\n                    const showPublisher = block.getAttribute(\"klascementShowPublisher\") == null || block.getAttribute(\"klascementShowPublisher\") == \"true\";\r\n                    const showMore = block.getAttribute(\"klascementShowMore\") == null || block.getAttribute(\"klascementShowMore\") == \"true\";\r\n                    for (let i = 0; i < objects.length; i++) {\r\n                        let object = objects[i];\r\n                        let klascementObject = $(\"<div id=\\\"klascementObject_\" + i + \"\\\" class=\\\"klascementObject\\\">\");\r\n                        klascementObject.append(\"<h4 class=\\\"klascementTitle\\\">\" + object.titel + \"<\/h4>\");\r\n                        if (showDescription) {\r\n                            if (showThumbnail) {\r\n                                klascementObject.append(\"<div class=\\\"klascementDescription\\\"><figure class=\\\"image\\\"><img height=\\\"100%\\\" src=\\\"\" + object.miniatuur + \"\\\" width=\\\"100%\\\" \/>\" +\r\n                                    \"  <figcaption style=\\\"text-align: right;\\\">\" +\r\n                                    \"    <div class=\\\"klascementTooltip\\\">\" + object.logobron.tekst +\r\n                                    \"      <span class=\\\"klascementTooltiptext\\\">\" + object.logobron.html + \"<\/span>\" +\r\n                                    \"    <\/div>\" +\r\n                                    \"  <\/figcaption>\" +\r\n                                    \"<\/figure>\" + object.omschrijving + \"<\/div>\");\r\n                            } else {\r\n                                klascementObject.append(\"<div class=\\\"klascementDescription\\\">\" + object.omschrijving + \"<\/div>\");\r\n                            }\r\n                        } else {\r\n                            if (showThumbnail) {\r\n                                klascementObject.append(\"<div class=\\\"klascementDescription\\\"><figure class=\\\"image\\\"><img height=\\\"100%\\\" src=\\\"\" + object.miniatuur + \"\\\" width=\\\"100%\\\" \/>\" +\r\n                                    \"  <figcaption style=\\\"text-align: right;\\\">\" +\r\n                                    \"    <div class=\\\"klascementTooltip\\\">\" + object.logobron.tekst +\r\n                                    \"      <span class=\\\"klascementTooltiptext\\\">\" + object.logobron.html + \"<\/span>\" +\r\n                                    \"    <\/div>\" +\r\n                                    \"  <\/figcaption>\" +\r\n                                    \"<\/figure>\" + \"<\/div>\");\r\n                            }\r\n                        }\r\n                        if (showType) {\r\n                            klascementObject.append(\"<div class=\\\"klascementType\\\"><p>Type       :\" + object.type + \"<\/p><\/div>\");\r\n                        }\r\n                        if (showPublisher) {\r\n                            if (object.uitgever.type == \"organisatie\") {\r\n                                klascementObject.append(\"<div class=\\\"klascementPublisher\\\"><p>Organisatie: \" + object.uitgever.organisatie + \"<\/p><\/div>\");\r\n                            } else if (object.uitgever.type == \"gebruiker\") {\r\n                                klascementObject.append(\"<div class=\\\"klascementPublisher\\\"><p>Uitgever   : \" + object.uitgever.gebruiker + \"<\/p><\/div>\");\r\n                            }\r\n                        }\r\n                        let klascementLinkContainer = $(\"<div class=\\\"klascementLinkContainer\\\">\");\r\n                        let klascementLinkParagraph = $(\"<p>\");\r\n                        let klascementLink = $(\"<a>\")\r\n                            .attr(\"href\", object.link)\r\n                            .attr(\"target\", \"_blank\")\r\n                            .append(\"Bekijk op KlasCement\");\r\n                        klascementLinkParagraph.append(klascementLink);\r\n                        klascementLinkContainer.append(klascementLinkParagraph);\r\n                        klascementObject.append(klascementLinkContainer);\r\n                        klascementObject.append(\"<div style=\\\"clear: both;\\\"><\/div>\");\r\n                        klascementContainer.append(klascementObject);\r\n                    }\r\n                    klascementContent.append(klascementContainer);\r\n                    debugger;\r\n                    $(id + \" .advice\").append(klascementContent);\r\n                }\r\n            }\r\n        }\r\n    );\r\n}\r\n\r\n\r\n"}};
		
    </script>
